{
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Database=webhooks;Username=username;Password=password"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "RateLimiting": {
    "PublicRequests": {
      "PermitLimit": 500,
      "Window": "00:01:00",
      "QueueLimit": 0
    },
    "AuthenticatedRequests": {
      "PermitLimit": 50,
      "Window": "00:01:00",
      "QueueLimit": 0
    }
  }
}
